    }

    .wf-checkwork .mb20 {
      margin-bottom: 20px;
    }

    .wf-checkwork .wf-button {
      display: inline-block;
      line-height: 1;
      white-space: nowrap;
      cursor: pointer;
      background-color: #628DB6;
      border: 1px solid #628DB6;
      color: #fff;
      -webkit-appearance: none;
      text-align: center;
